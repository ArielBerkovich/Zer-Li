<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="404.0" prefWidth="750.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="order.ViewCustomProductsInOrder">
   <children>
      <Label layoutX="247.0" layoutY="15.0" text="Custom Product in Order">
         <font>
            <Font size="22.0" />
         </font>
      </Label>
      <TableView fx:id="productsTable" layoutX="15.0" layoutY="60.0" prefHeight="290.0" prefWidth="720.0">
        <columns>
          <TableColumn fx:id="nameCol" prefWidth="193.0" text="Name" />
            <TableColumn fx:id="typeCol" prefWidth="105.0" text="Type" />
            <TableColumn fx:id="colorCol" prefWidth="124.0" text="Color" />
            <TableColumn fx:id="greetingCardCol" prefWidth="215.0" text="Greeting Card" />
            <TableColumn fx:id="viewCol" text="View" />
        </columns>
      </TableView>
      <Button fx:id="closeBtn" layoutX="339.0" layoutY="365.0" mnemonicParsing="false" onAction="#onClose" prefWidth="60.0" text="Close" />
   </children>
</AnchorPane>
