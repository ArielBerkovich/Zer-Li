<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="491.0" prefWidth="663.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="catalog.CatalogGUI">
   <children>
      <TableView fx:id="catalogTable" layoutX="15.0" layoutY="56.0" prefHeight="372.0" prefWidth="633.0">
        <columns>
          <TableColumn fx:id="imageCol" prefWidth="82.0" text="Image" />
          <TableColumn fx:id="nameCol" prefWidth="163.0" text="Name" />
            <TableColumn fx:id="typeCol" prefWidth="91.0" text="Type" />
            <TableColumn fx:id="colorCol" prefWidth="64.0" text="Color" />
            <TableColumn fx:id="priceCol" prefWidth="109.0" text="Price" />
            <TableColumn fx:id="checkboxCol" prefWidth="135.0" text="Select to Buy" />
        </columns>
      </TableView>
      <Label layoutX="269.0" text="Catalog">
         <font>
            <Font size="37.0" />
         </font>
      </Label>
      <Button fx:id="backBtn" layoutX="15.0" layoutY="450.0" mnemonicParsing="false" onAction="#onBack" prefWidth="60.0" text="Back" />
      <Button fx:id="createOrderBtn" layoutX="289.0" layoutY="450.0" mnemonicParsing="false" onAction="#onCreateOrder" text="Create Order" />
   </children>
</AnchorPane>
