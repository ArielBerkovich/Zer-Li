<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="434.0" prefWidth="458.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="prototype.ShowProductController">
   <center>
      <AnchorPane prefHeight="440.0" prefWidth="440.0" BorderPane.alignment="CENTER">
         <children>
            <TableView fx:id="productView" layoutX="39.0" layoutY="60.0" prefHeight="327.0" prefWidth="383.0">
              <columns>
                <TableColumn fx:id="ProductIDCol" prefWidth="87.0" text="Product ID" />
                <TableColumn fx:id="ProductNameCol" prefWidth="187.0" text="Product Name" />
                  <TableColumn fx:id="ProductTypeCol" prefWidth="108.0" text="Product Type" />
              </columns>
            </TableView>
            <Label fx:id="productLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="171.0" layoutY="15.0" prefHeight="11.0" prefWidth="114.0" text="Products">
               <font>
                  <Font size="26.0" />
               </font>
            </Label>
            <Button fx:id="buttonUpdate" layoutX="362.0" layoutY="400.0" mnemonicParsing="false" onAction="#OnUpdate" prefWidth="60.0" text="Update" />
            <Button fx:id="buttonRefresh" layoutX="201.0" layoutY="400.0" mnemonicParsing="false" onAction="#onRefresh" prefWidth="60.0" text="Refresh" />
         </children>
      </AnchorPane>
   </center>
</BorderPane>
