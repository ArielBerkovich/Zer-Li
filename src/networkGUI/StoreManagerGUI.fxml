   <children>
      <Label fx:id="welcomeLbl" layoutX="55.0" layoutY="17.0" text="Welcome, Store manager" />
      <Button fx:id="viewReportsBtn" layoutX="54.0" layoutY="56.0" mnemonicParsing="false" onAction="#onViewReports" prefHeight="51.0" prefWidth="170.0" text="View store's reports" />
      <Button fx:id="createCustomerBtn" layoutX="55.0" layoutY="123.0" mnemonicParsing="false" onAction="#onCreateCustomer" prefHeight="51.0" prefWidth="170.0" text="Create new customer" />
      <Button fx:id="logOutBtn" layoutX="15.0" layoutY="205.0" mnemonicParsing="false" onAction="#onLogOut" text="Log out" />
      <Separator layoutX="3.0" layoutY="198.0" prefHeight="4.0" prefWidth="278.0" />
   </children>
</AnchorPane>
